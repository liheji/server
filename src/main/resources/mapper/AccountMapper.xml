<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="top.yilee.server.mapper.AccountMapper">
    <resultMap id="BaseResultMap" type="top.yilee.server.pojo.Account">
        <id property="id" column="id"/>
        <result property="username" column="username"/>
        <result property="password" column="password"/>
        <result property="mobile" column="mobile"/>
        <result property="email" column="email"/>
        <result property="isEnabled" column="is_enabled"/>
        <result property="isSuperuser" column="is_superuser"/>
        <result property="lastLogin" column="last_login"/>
        <result property="updateTime" column="update_time"/>
        <result property="createTime" column="create_time"/>
        <result property="version" column="version"/>
    </resultMap>

    <sql id="Base_Column_List">
        id
        ,username,password,
        mobile,email,is_enabled,is_superuser,
        last_login,update_time,create_time,
        version
    </sql>
</mapper>
