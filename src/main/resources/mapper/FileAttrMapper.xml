<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="top.liheji.server.mapper.FileAttrMapper">

    <cache-ref namespace="top.liheji.server.mapper.FileAttrMapper"/>

    <resultMap id="BaseResultMap" type="top.liheji.server.pojo.FileAttr">
        <id property="id" column="id" jdbcType="INTEGER"/>
        <result property="fileName" column="file_name" jdbcType="VARCHAR"/>
        <result property="fileSize" column="file_size" jdbcType="BIGINT"/>
        <result property="fileHash" column="file_hash" jdbcType="VARCHAR"/>
        <result property="updateTime" column="update_time" jdbcType="TIMESTAMP"/>
        <result property="createTime" column="create_time" jdbcType="TIMESTAMP"/>
        <result property="version" column="version" jdbcType="INTEGER"/>
        <result property="accountId" column="account_id" jdbcType="INTEGER"/>
        <association property="account"
                     column="account_id"
                     fetchType="lazy"
                     select="top.liheji.mapper.AccountMapper.selectById">
        </association>
    </resultMap>

    <sql id="Base_Column_List">
        id,file_name,file_size,
        file_hash,update_time,create_time,
        version,account_id
    </sql>
</mapper>
