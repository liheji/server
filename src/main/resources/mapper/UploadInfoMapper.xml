<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="top.liheji.server.mapper.UploadInfoMapper">

    <cache-ref namespace="top.liheji.server.mapper.UploadInfoMapper"/>

    <resultMap id="BaseResultMap" type="top.liheji.server.pojo.UploadInfo">
        <id property="id" column="id"/>
        <result property="fileName" column="file_name"/>
        <result property="createTime" column="create_time"/>
        <result property="fileInfoId" column="file_info_id"/>
        <result property="accountId" column="account_id"/>
    </resultMap>

    <sql id="Base_Column_List">
        id
        ,file_name,create_time,
        file_info_id,account_id
    </sql>
</mapper>
